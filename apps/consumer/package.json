{
  "name": "@remoola/consumer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "openapi:pull": "curl -s http://localhost:3001/api-json -o src/openapi.json",
    "openapi:gen": "openapi --input src/openapi.json --output src/generated/api --client fetch --useOptions --useUnionTypes",
    "openapi": "npm run openapi:pull && npm run openapi:gen"
  },
  "dependencies": {
    "@remoola/openapi": "^0.1.0",
    "@remoola/ui": "^0.1.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swr": "^2.3.6",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@remoola/eslint-config": "^0.0.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.2",
    "openapi-typescript-codegen": "^0.29.0",
    "tailwindcss": "^4",
    "typescript": "^5.5.4"
  }
}
